# API Base URL - Vite uses VITE_ prefix
VITE_API_URL=http://localhost:5000/api

# Socket (if using WebSockets)
VITE_SOCKET_URL=http://localhost:5173

# OAuth / Third-Party Keys
# (Uncomment and fill in if applicable)
# VITE_GOOGLE_CLIENT_ID=your-google-client-id
# VITE_FACEBOOK_APP_ID=your-facebook-app-id

# Feature Flags
VITE_ENABLE_BETA=false
VITE_USE_MOCK=false

# Environment
VITE_ENVIRONMENT=development

# Analytics / Monitoring
# (Uncomment and fill in if applicable)
# VITE_SENTRY_DSN=https://examplePublicKey@o0.ingest.sentry.io/0

# Optional: configure timeouts (ms)
VITE_REQUEST_TIMEOUT=30000