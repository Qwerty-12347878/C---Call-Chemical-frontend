# API Base URL
REACT_APP_API_URL=http://localhost:5173/api

# Socket (if using WebSockets)
REACT_APP_SOCKET_URL=http://localhost:5173

# OAuth / Third-Party Keys
# (Uncomment and fill in if applicable)
# REACT_APP_GOOGLE_CLIENT_ID=your-google-client-id
# REACT_APP_FACEBOOK_APP_ID=your-facebook-app-id

# Feature Flags
REACT_APP_ENABLE_BETA=false
REACT_APP_USE_MOCK=false

# Environment
REACT_APP_ENVIRONMENT=development

# Analytics / Monitoring
# (Uncomment and fill in if applicable)
# REACT_APP_SENTRY_DSN=https://examplePublicKey@o0.ingest.sentry.io/0

# Optional: configure timeouts (ms)
REACT_APP_REQUEST_TIMEOUT=30000
